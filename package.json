{
  "name": "nuxt-typing-wl",
  "repository": "https://github.com/ampu/nuxt-typing-wl/",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "gh-pages": "npm run generate:gh-pages && npm run deploy:gh-pages",
    "generate:gh-pages": "GH_PAGES=1 nuxt generate",
    "deploy:gh-pages": "touch .output/public/.nojekyll && gh-pages -t -d .output/public"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "18.16.18",
    "gh-pages": "5.0.0",
    "nuxt": "3.6.1",
    "sass": "1.63.4",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "vue": "3.3.4",
    "vuetify": "3.3.6"
  }
}
